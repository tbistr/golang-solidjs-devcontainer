{
    "name": "golang-vsode-devcontainer",
    "build": {
        "dockerfile": "Dockerfile"
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "dbaeumer.vscode-eslint",
                "esbenp.prettier-vscode",
                "golang.Go"
            ]
        }
    },
    "postStartCommand": "cd ./client && npm i",
    "remoteUser": "vscode"
}
